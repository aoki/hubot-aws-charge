{
  "name": "hubot-aws-charge",
  "version": "1.0.0",
  "description": "Post to chatroom AWS-charges per 4 hours",
  "main": "scriptsgi/aws-charge.js",
  "bin": "lib/external-script.js",
  "scripts": {
    "start": "coffee --nodejs --debug $(npm bin)/hubot -a shell -n bot",
    "test": "istanbul cover _mocha",
    "lint": "eslint scripts/*",
    "coveralls": "istanbul-coveralls",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ringohub/hubot-aws-charge.git"
  },
  "keywords": [
    "hubot",
    "aws"
  ],
  "author": "Yoshiki Aoki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ringohub/hubot-aws-charge/issues"
  },
  "homepage": "https://github.com/ringohub/hubot-aws-charge#readme",
  "dependencies": {
    "aws-sdk": "2.9.0",
    "cron": "1.2.1",
    "date-utils": "1.2.21"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.4.0",
    "coffee-script": "1.12.3",
    "eslint": "3.14.1",
    "hubot": "2.19.0",
    "istanbul": "0.4.5",
    "istanbul-coveralls": "1.0.3",
    "mocha": "3.2.0",
    "power-assert": "1.4.2"
  }
}
